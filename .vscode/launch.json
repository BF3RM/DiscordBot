{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Bot",
            "program": "${workspaceFolder}/src/index.ts",
            "runtimeArgs": ["-r", "dotenv/config"],
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outputCapture": "std",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}